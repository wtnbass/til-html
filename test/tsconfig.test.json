{
  "compilerOptions": {
    "noEmit": true,
    "target": "es6",
    "module": "es6",
    "lib": ["es6", "dom"],
    "moduleResolution": "node",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["./**/*.ts"]
}
